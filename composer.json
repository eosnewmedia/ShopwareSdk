{
  "name": "enm/shopware-sdk",
  "description": "An implementation for accessing the Shopware api",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=7.0",
    "psr/http-message": "^1.0"
  },
  "suggest": {
    "guzzlehttp/guzzle": "Needed to use default implementation",
    "jms/serializer": "Needed to use default implementation"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "guzzlehttp/guzzle": "^6.2",
    "jms/serializer": "^1.4"
  },
  "authors": [
    {
      "name": "Dirk Heyka",
      "email": "heyka@eosnewmedia.de"
    },
    {
      "name": "Nicklas Reincke",
      "email": "reincke@eosnewmedia.de"
    },
    {
      "name": "Philipp Marien",
      "email": "marien@eosnewmedia.de"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Enm\\ShopwareSdk\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Enm\\ShopwareSdk\\Tests\\": "tests"
    }
  }
}
